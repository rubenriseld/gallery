{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{css, svg, js, ts, png, jpeg, jpg, ico}",
            "/api/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ]
}